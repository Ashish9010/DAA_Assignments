#include <iostream>
using namespace std;

void floydWarshall(int graph[][4],int n) {
  for (int k = 0; k < n; k++) {
    for (int i = 0; i < n; i++) {
      for (int j = 0; j < n; j++) {
        if (graph[i][j] > graph[i][k] + graph[k][j])
          graph[i][j] = graph[i][k] + graph[k][j];
      }
    }
  }
  printMatrix((int *)graph,n);
}

void printMatrix(int *matrix,int n) {
  
}

int main() {
    int n,INF = 100000;
    cout<<"Enter the number of nodes : ";
    cin>>n;

    int arr[n][n] = {{1, 2, 3, 10},
                    {4, 5, 6, 11},
                    {7, 8, 9, 12},
                    {13, 14, 15,16}};
    floydWarshall(arr,n);

    for (int i = 0; i < n; i++) {
      cout<<"\nMinimum Cost With Respect to Node:"<<i<<endl;
      for (int j = 0; j < n; j++) {
        if (arr[i][j] == 100000)
          cout<<"INF ";
        else
          cout<<arr[i][j]<<" ";
      }
      cout<<"\n";
    }
}